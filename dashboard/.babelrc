{
  "presets": [
	  ["es2015",  { "loose" : true }],
	  "es2016",
	  "react"
  ],
  "plugins": [
		"react-hot-loader/babel",
	  "transform-object-rest-spread",
	  "syntax-trailing-function-commas",
	  "transform-class-properties",
	  "transform-export-extensions",
		"transform-react-jsx-img-import",
		"jsx-control-statements"
  ]
}